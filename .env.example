# Configuración de credenciales para Moodle
# Renombra este archivo a .env y completa los valores

# URL base de tu instancia de Moodle (sin slash al final)
MOODLE_URL=https://tu-moodle.com

# Credenciales de acceso
MOODLE_USERNAME=tu_usuario
MOODLE_PASSWORD=tu_contraseña

# Configuración del navegador
# Establecer a "True" para ejecutar en modo invisible (sin interfaz gráfica)
HEADLESS=False

# Tamaño de la ventana del navegador
VIEWPORT_WIDTH=1920
VIEWPORT_HEIGHT=1080 